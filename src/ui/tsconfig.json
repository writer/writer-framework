{
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"lib": ["dom", "es6"],
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true,
		"importHelpers": true,
		"isolatedModules": true,
		"noEmit": true,
		"paths": {
			"@/*": ["./src/*"]
		}
	}
}
